<mvc:View
	controllerName="ui5.typescript.helloworld.controller.App"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:cc="ui5.typescript.helloworld.control"
>
	<App id="app">
		<!-- <cc:MyControl text="Hello World from the custom control!"/> -->
		<MessagePage
			title="Hello World"
			text="This is a UI5 TypeScript Hello World app"
			icon="sap-icon://accept"
			id="page"
			description=""
		>
			<buttons>
				<Button
					text="Say Hello"
					press="sayHello"
				/>
			</buttons>
		</MessagePage>
	</App>
</mvc:View>